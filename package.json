    "prettier": "3.5.3",
    "vite": "^6.2.4",
    "vite-plugin-pwa": "^1.0.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vite-plugin-vuetify": "^2.1.1"
  }
}
